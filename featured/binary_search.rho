!input ../lang
@import lang[trace]
@import lang.cmp[cmp_int32 = int32]
@import lang.list[create, push, get, binary_search, Found, InsertAt]
@import lang.typing[True, False]

let xs = create()
push(xs, 1)
push(xs, 2)
push(xs, 3)
push(xs, 4)
push(xs, 11)
push(xs, 12)
push(xs, 13)
push(xs, 14)

trace(binary_search(xs, 1, cmp_int32))
trace(binary_search(xs, 12, cmp_int32))
trace(binary_search(xs, 5, cmp_int32))