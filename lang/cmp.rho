!input typing
@package lang.cmp
@import lang.typing[True, False]

let LessThan = type
@export LessThan
let Equal = type
@export Equal
let GreaterThan = type
@export GreaterThan

let int32 = func(a, b) {
    match a < b
        case True new LessThan
        case False match a > b
            case True new GreaterThan
            case False new Equal
}
@export int32